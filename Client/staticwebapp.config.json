{
    "routes": [
        {
            "route": "/fetchdata",
            "allowedRoles": ["authenticated"]
        },
        {
            "route": "/api/*",
            "allowedRoles": ["authenticated"]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}",
            "/css/*"
        ]
    }
}